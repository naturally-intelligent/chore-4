; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Chore Engine 4"
config/description="Chore Engine 4"
run/main_scene="res://menus/launch.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.4")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/use_filter=false
boot_splash/image="res://icon.png"
config/icon="res://icon.png"

[autoload]

debug="*res://engine/debug.gd"
math="*res://engine/math.gd"
util="*res://engine/util.gd"
settings="*res://core/settings.gd"
game="*res://core/game.gd"
dev="*res://core/dev.gd"
audio="*res://engine/audio.tscn"
root="*res://engine/root.tscn"
scenes="*res://engine/scenes.gd"
menus="*res://engine/menus.gd"

[debug]

gdscript/warnings/unused_variable=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/unused_argument=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="viewport"

[filesystem]

import/blender/enabled=false

[gui]

theme/default_theme_scale=2.0
theme/custom="res://art/chore_theme.tres"
theme/custom_font="res://art/fonts/publicsans/PublicSans-Regular.ttf"
theme/default_font_antialiasing=0

